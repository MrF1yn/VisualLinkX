{
	"name": "visuallinkx",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsc src/node-server.ts --target esnext --allowSyntheticDefaultImports --moduleResolution nodenext --module NodeNext --outDir build/",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "node -r dotenv/config build/node-server.js"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/express": "^4.17.21",
		"@types/multer": "^1.4.11",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"bits-ui": "^0.14.0",
		"body-parser": "^1.20.2",
		"clsx": "^2.1.0",
		"dotenv": "^16.3.2",
		"embla-carousel": "^8.0.0-rc19",
		"embla-carousel-autoplay": "^8.0.0-rc19",
		"embla-carousel-svelte": "^8.0.0-rc19",
		"express": "^4.18.2",
		"livekit-client": "^1.15.10",
		"livekit-server-sdk": "^1.2.7",
		"mode-watcher": "^0.1.2",
		"multer": "^1.4.5-lts.1",
		"radix-icons-svelte": "^1.2.1",
		"svelte-awesome": "^3.3.1",
		"svelte-sonner": "^0.3.11",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.20"
	}
}
